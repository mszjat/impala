---- QUERY
USE $DATABASE;
DROP TABLE IF EXISTS gbk_big;
CREATE TABLE gbk_big (
  name1 string, name2 string, name3 string, name4 string, name5 string
  ) STORED AS TEXTFILE;
ALTER TABLE gbk_big SET SERDEPROPERTIES('serialization.encoding'='gbk');
REFRESH gbk_big;
INSERT OVERWRITE TABLE gbk_big SELECT * FROM functional.gbk_big_utf8;
(select * from functional.gbk_big_utf8 except select * from gbk_big)
union (select * from gbk_big except select * from functional.gbk_big_utf8)
